{
  "name": "my-log",
  "version": "0.0.1",
  "dependencies": {
    "formik": "^2.2.9",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "lint": "npm run lint:es; npm run lint:etc; npm run lint:style",
    "lint:fix": "npm run lint:es:fix; npm run lint:etc:fix; npm run lint:style:fix",
    "lint:es": "eslint '**/*.{js,ts,tsx}'",
    "lint:es:fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "lint:etc": "prettier '**/*.{json,yaml,md}' --check",
    "lint:etc:fix": "prettier '**/*.{json,yaml,md}' --write",
    "lint:style": "stylelint '**/*.{js,ts,tsx}'",
    "lint:style:fix": "stylelint './**/*.{js,ts,tsx}' --fix"
  },
  "devDependencies": {
    "@titicaca/eslint-config-triple": "^3.0.3",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "typescript": "^4.5.5"
  },
  "prettier": "@titicaca/eslint-config-triple/prettierrc"
}
